<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <listener>
    <listener-class>students.ContextListener</listener-class>
  </listener>

  <servlet>
    <servlet-name>aaaaa</servlet-name>
    <servlet-class>students.HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>hello</servlet-name>
    <servlet-class>students.HelloServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>student</servlet-name>
    <servlet-class>students.StudentsServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>aaaaa</servlet-name>
    <url-pattern>/hiiiiiii</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>hello</servlet-name>
    <url-pattern>/hi</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>student</servlet-name>
    <url-pattern>/student</url-pattern>
  </servlet-mapping>
</web-app>
